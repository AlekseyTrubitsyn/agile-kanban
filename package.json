    "moment": "^2.22.2",
    "node-sass-chokidar": "^1.3.3",
    "npm": "^6.3.0",
    "npm-run-all": "^4.1.3",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-beautiful-dnd": "^8.0.7",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
